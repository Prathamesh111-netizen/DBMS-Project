<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Musical-Z</title>
    <link
      href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <script
      type="text/javascript"
      src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"
    ></script>
    <link
      rel="stylesheet"
      href="https://fonts.google.com/icons?selected=Material%20Icons%3Aplay_arrow%3A"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0-alpha.6/css/bootstrap.min.css"
    />
    <!-- <link rel="stylesheet" href="css/profile.css"> -->
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.2/css/all.css"
    />
    <link rel="stylesheet" href="css/navbar.css" />
    <style></style>
  </head>

  <body oncontextmenu="return false" class="snippet-body">
    <link
      rel="stylesheet"
      href="https://cdn.datatables.net/1.10.2/css/jquery.dataTables.min.css"
    />
    <script
      type="text/javascript"
      src="https://cdn.datatables.net/1.10.2/js/jquery.dataTables.min.js"
    ></script>

    <nav class="navbar navbar-expand-custom navbar-mainbg">
      <a class="navbar-brand navbar-logo" href="#">Navbar</a>
      <button
        class="navbar-toggler"
        type="button"
        aria-controls="navbarSupportedContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <i class="fas fa-bars text-white"></i>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ml-auto">
          <div class="hori-selector">
            <div class="left"></div>
            <div class="right"></div>
          </div>
          <li class="nav-item active">
            <a class="nav-link" href="/song"><i class=""></i>Dashboard</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/dashboard"><i class=""></i>My Profile</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/likes"><i></i>Information Zone</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/"><i></i>Log-out</a>
          </li>
        </ul>
      </div>
    </nav>
    <!-- partial -->
    <script src="js/navbar.js"></script>
    <iframe
      id="playframe"
      src="https://open.spotify.com/embed/track/4evmHXcjt3bTUHD1cvny97?utm_source=generator"
      width="1500"
      height="350"
      frameborder="0"
      allowtransparency="true"
      allow="encrypted-media"
    ></iframe>
    <br /><br />
    <div class="container">
      <table
        id="example"
        class="table table-striped table-bordered"
        style="width: 100%"
      >
        <thead>
          <tr>
            <th>#</th>
            <th>Song_Title</th>
            <th>Length</th>
            <th>Genre</th>
            <th>Language</th>
            <th>Actions</th>
          </tr>
        </thead>

        <tbody>
          <% if(data.length){ for(var i = 0;i < data.length;i++) { %>
          <tr class="tupleentry">
            <td><%=(i+1)%></td>
            <td><%=data[i].Song_Title%></td>
            <td><%=data[i].Length%></td>
            <td>
              <% if(data[i].Genre == 4){ %>
                  Rock<% }%>
              <% if(data[i].Genre == 5){ %>
                  Classical<% }%>
              <% if(data[i].Genre == 6){ %>
                  Romantic<% }%>
              <% if(data[i].Genre == 7){ %>
                  Dance<% }%>
              <% if(data[i].Genre == 8){ %>
                  Pop<% }%>
            </td>
            <td>
              <% if(data[i].Language == 9){ %>
                  English<% }%>
              <% if(data[i].Language == 10){ %>
                  Marathi<% }%>
              <% if(data[i].Language == 11){ %>
                  Gujrathi<% }%>
              <% if(data[i].Language == 12){ %>
                  Hindi<% }%>
              <% if(data[i].Language == 13){ %>
                  Spanish<% }%>
              <% if(data[i].Language == 14){ %>
                  Japanese<% }%>
              <% if(data[i].Language == 15){ %>
                  Sanskrit<% }%>
              
            </td>
            <td >
              <a onclick="playsong('<%=data[i].Share_Link%>')" href="#" class="view" title="Play song" data-toggle="tooltip"
                ><i class="material-icons">&#xE039;</i></a
              >

              <form action="/song" method="post">
                <input type="text" name = "Playlist_ID" placeholder="Playlist" size="8">
                <input type="text" name = "User_ID" placeholder="User" size="8">
                <input type="text" name = "Song_ID" placeholder="Song" size="8">
                <input type="text" name = "Playlist_Name" placeholder="Name" size="8">
                <button type="submit" value = "Add" style="border:none;">
                  <i class="material-icons">&#xE02E;</i></button>
              </form>

              <a href="#" class="edit" title="Add to Playlist" data-toggle="tooltip"
                ></a
              >

              <form action="/song" method="post">
                <input type="text" name = "Song_ID" placeholder="Input Song Id" size="10">
              <button type="submit" value = "like" style="border:none;"><i class="material-icons">&#xE8DC;</i></button>
              <!-- <button type="submit"  value = "dislike" style="border:none;"><i class="material-icons">&#xE8DB;</i></button> -->

            </form>
              <a href="#" class="delete" title="Comment" data-toggle="tooltip"
                ><i class="material-icons">&#xE3C9;</i></a
              >
            </td>
          </tr>
          <% } }else{ %>
          <tr>
            <td colspan="3">No data</td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>

    <script
      type="text/javascript"
      src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"
    ></script>
    <script type="text/javascript" src=""></script>
    <script type="text/javascript" src=""></script>
    <script type="text/Javascript">
      $(document).ready(function() {
      $('#example').DataTable();});
    </script>
    <script>
     function playsong(Share_Link){
        var songplaying = document.getElementById(playframe);
        playframe.src = Share_Link;
      }
    </script>
  </body>
</html>
